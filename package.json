{
    "name": "wyvr-generator",
    "version": "0.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "tsc && node ./built/index.js",
        "watch": "tsc && env WYVR_ENV=debug nodemon --ignore test --ignore imported --ignore data --ignore pub --ignore state -e js,ts ./built/index.js",
        "dev": "tsc && env WYVR_ENV=dev node ./built/index.js",
        "serve": "npx http-server ./pub",
        "test": "nyc -x 'config/**/*' -x 'test/**/*' mocha -R dot './test/**/*.js'",
        "watch-test": "nodemon --watch src --watch lib --watch test -e js,ts --exec 'npm run test'"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "ansi-colors": "^4.1.1",
        "fs-extra": "^10.0.0",
        "glob": "^7.1.7",
        "http-server": "^0.12.3",
        "mocha": "^8.4.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "rollup": "^2.48.0",
        "stream-json": "^1.7.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.0.0",
        "locate-character": "^2.0.5",
        "magic-string": "^0.25.7",
        "module-alias": "^2.2.2",
        "rollup-plugin-svelte": "^7.1.0",
        "svelte": "^3.38.2",
        "typescript": "^4.2.4"
    },
    "_moduleAliases": {
        "@root": ".",
        "@lib": "built",
        "@config": "config",
        "@src": "src"
    }
}
