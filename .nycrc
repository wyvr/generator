{
    "check-coverage": true,
    "all": true,
    "extension": [
        ".js",
        ".ts"
    ],
    "include": [
        "lib/**/*.js"
    ],
    "exclude": [
        "lib/resource/**/*.*",
        "test/**",
        "node_modules/**"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "coverage"
}