{
    "root": true,
    "plugins": [],
    "extends": ["eslint:recommended"],
    "rules": {
        "no-console": 2
    },
    "env": {
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "window": true,
        "document": true,
        "__I18N__": true,
        "fetch": true,
        "CustomEvent": true,
        "getStack": true
    }
}
